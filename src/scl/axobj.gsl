.function method_body ()
    safe = 0
    is_cpp_object = 0
    for rule
        for object.rule as orule where orule.name = rule.name
            for type where value = "activex_cpp_data" | value = "activex_cpp"
                if defined (threadsafe)
                    safe = threadsafe
                endif
                if defined (use_cpp_object)
                    is_cpp_object = use_cpp_object
                endif
            endfor
        endfor
    endfor

    for rule
        for object.rule as orule where orule.name = rule.name
            for type where value = "activex_cpp_data"
>$(type.:)
            endfor
        endfor
    endfor
    method_header (safe, is_cpp_object)
    for rule
        for object.rule as orule where orule.name = rule.name
            for type where value = "activex_cpp"
>$(type.:)
            endfor
        endfor
    endfor
    method_footer (safe, is_cpp_object)
.endfunction

.macro method_footer (safe, is_cpp_object)
.   if debug ?= 1
    tracefile = fopen ("c:\\\\scl.log", "a");
    fprintf (tracefile, "Exit C$(object.name:)::$(name:)\\n");
    fflush (tracefile);
    fclose (tracefile);
.   endif
.   if !safe
.#          unlock_function ();
            UNLOCK;
.endif
.if is_cpp_object ?= 0
    }
    __except (log2event ("Error in method C$(object.name:)::$(name:)"), 1)
    {
          UNLOCK;
    }
.endif
.   if !safe

      }
    else
      {
        char
            buffer [1024];
        sprintf (buffer, "Mutex error C$(object.name:)::$(name:):\\nmutex = %X\\nerror code: %ld",
                 m_mutex, GetLastError ());

        UNLOCK; 
        log2event (buffer);
        return (S_FALSE);
      }     
.   endif
.endmacro

------------------------------------------------------------------------
function get_c_type (type, pointer)
------------------------------------------------------------------------
    if type = "string"
        c_type = "char"
    elsif type = "bool"
        c_type = "BOOL"
    else
        c_type = type
    endif
    if pointer ?= "yes"
        c_type += " *"
    endif
endfunction
-
------------------------------------------------------------------------
function get_doc_type (type)
------------------------------------------------------------------------
    if type = "bstr" | type = "BSTR"
        doc_type = "String"
    elsif type = "bool" | type = "BOOL"
        doc_type = "Boolean"
    else
        doc_type = type
    endif
endfunction
