------------------------------------------------------------------------
function CPP_PARAM (nb)
------------------------------------------------------------------------
    param = ""
    for parameter
        param += "$(type:) $(name:)"
        if index (parameter) < nb
           param += ", "
        endif
    endfor
    for return
        if length (param) > 0
            param += ", "
        endif
        param += "$(type:) *pVal"
    endfor
    if length (param) = 0
        param = "void"
    endif
endfunction

